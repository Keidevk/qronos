{
  "expo": {
    "name": "Qronos",
    "slug": "qronos",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "qronos",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 6,
      "package": "com.sebas.qronos",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permite $(PRODUCT_NAME) acceder a la cámara para escanear códigos QR.",
          "microphonePermission": "Permite $(PRODUCT_NAME) grabar audio al capturar videos.",
          "recordAudioAndroid": true
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#E6F4FE",
          "dark": {
            "image": "./assets/images/icon.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Permita usar Face ID por Qronos para mayor seguridad."
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "60bafa47-7ad8-4d14-b6c8-9b05333a55f7"
      }
    },
    "owner": "qronos"
  }
}